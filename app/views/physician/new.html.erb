<%= link_to("Back Button", physician_index_path, :class => 'back-link')%>
<div class="author new">

  <h1>Create Physician</h1>
  <%= form_for(@physician,:url => physician_index_path,:method => 'post')do |f| %>
  <table>
  <tr>
    <th> Enter Physician Name </th>
    <td><%= f.text_field(:name)%></td>
    </tr>
  </table>
  <div class="form-buttons">
    <%= f.submit("Create Physician")%>
  </div>
  <%end%>
</div>