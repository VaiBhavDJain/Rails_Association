<%= link_to("Back Button", employee_index_path, :class => 'back-link')%>
<div>
  <h1>Delete Employee</h1>
  <%= form_for(@employee,:url => employee_path(@employee),:method => 'delete')do |f| %>
  <p>Are you sure you want to delete this Teacher</p>
  
  <p class="reference-name"><%= @employee.name%></p>

  <div class="form-buttons">
    <%= f.submit("Delete Employee")%>
  </div>
  <%end%>
</div>