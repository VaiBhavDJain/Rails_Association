<%= link_to("Back", supp_index_path, :class => 'back-link')%>
<div class="supplier delete">
  <h1>Delete Supplier</h1>
  <%= form_for(@supplier,:url => supp_path(@supplier),:method => 'delete')do |f| %>
  <p>Are you sure you want to delete this Physician</p>
  
  <p class="reference-name"><%= @supplier.name%></p>


  <div class="form-buttons">
    <%= f.submit("Delete")%>
  </div>
  <%end%>
</div>