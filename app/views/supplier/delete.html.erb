<%= link_to("Back", supplier_index_path, :class => 'back-link')%>
<div class="supplier delete">
  <h1>Delete Author</h1>
  <%= form_for(@supplier,:url => supplier_path(@supplier),:method => 'delete')do |f| %>
  <p>Are you sure you want to delete this Supplier</p>
  
  <p class="reference-name"><%= @supplier.name%></p>


  <div class="form-buttons">
    <%= f.submit("Delete")%>
  </div>
  <%end%>
</div>